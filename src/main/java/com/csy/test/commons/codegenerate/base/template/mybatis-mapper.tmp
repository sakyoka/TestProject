<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="${daoPath}">
    
    <!-- 获取集合数据 -->
    <select id="${findListId}" resultType="${javaBeanPath}">
        ${findListSql}
    </select>
    
    <!-- 获取一条 -->
    <select id="${getOneId}" resultType="${javaBeanPath}">
        ${getOneSql}
    </select>
    
    <!--保存 -->
    <insert id="${insertId}">
        ${insertSql}
    </insert>
    
    <!-- 修改 -->
    <update id="${updateId}">
        ${updateSql}
    </update>
    
    <!-- 删除 -->
    <delete id="${deleteId}">
        ${deleteSql}
    </delete>
</mapper>